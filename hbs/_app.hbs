         <div class="col-sm-12 col-md-8 tabla-banner">
          <table class="table record banner-table">
            <tbody>
              <tr class="table-banner-tr">
                <td><h1>Nota</h1><span>{{_created.name}} {{_created.folio}}</span></td>
                {{#if persona.esPaciente}}
                <td><h1>Fecha / Hora Atención</h1><span>{{#if _updated.date}}{{date _updated.date "DD/MMM/YYYY h:mm a"}}{{else}}{{date _created.date "DD/MMM/YYYY h:mm a"}}{{/if}}</span></td>
                <td><h1>Número Expediente</h1><span>{{persona.clave}}</span></td>
                <td><h1>Nombre Completo</h1><span>{{persona.nombreCompleto}}</span></td>
                {{else}}
                <td><h1>Fecha / Hora Emisión</h1><span>{{#if _updated.date}}{{date _updated.date "DD/MMM/YYYY h:mm a"}}{{else}}{{date _created.date "DD/MMM/YYYY h:mm a"}}{{/if}}</span></td>
                <td colspan="2"><h1>Destino Final</h1><span>{{persona.nombreCompleto}}</span></td>
                {{/if}}
              </tr>
              <tr class="table-banner-tr">
                {{#if persona.esPaciente}}
                <td colspan="2"><h1>Solicitud / Origen</h1><span>{{#if _created._request}}{{_created._request}}{{else}}{{_parent.name}}{{/if}}</span></td>
                <td><h1>Fecha Nacimiento / Edad Actual</h1><span>{{date persona.fechaNacimiento "DD/MMM/YYYY"}} ({{persona._edadActual}})</span></td>
                <td><h1>Género / Tipo Sanguíneo</h1><span>{{persona._genero}}{{#if persona.tipoSanguineo}}, {{persona._tipoSanguineo}}{{/if}}</span></td>
                {{else}}
                <td class="last-line" colspan="4"><h1>Solicitud / Origen</h1><span>{{#if _created._request}}{{_created._request}}{{else}}{{_parent.name}}{{/if}}</span></td>
                {{/if}}
              </tr>
              {{#if persona.esPaciente}}
              <tr class="table-banner-tr">
                <td><h1>Fecha Ingreso / Episodio</h1><span>{{date contexto.ingreso.fechaHora "DD/MMM/YYYY hh:mma"}}{{#if contexto.episodio}} {{contexto.episodio}}{{/if}}</span></td>
                <td><h1>Diagnóstico Ingreso</h1><span>{{contexto.ingreso._seleccionarDiagnostico}}</span></td>
                <td><h1>Médico Ingreso</h1><span>{{contexto._medico}}</span></td>
                <td><h1>Servicio / Cama Ingreso</h1><span>{{contexto._servicio}}{{#if contexto.cama}} {{contexto._cama}}{{/if}}</span></td>
              </tr>
              <tr class="table-banner-tr">
                <td colspan="2"><h1>Alergias</h1><span>{{persona._alergia}}</span></td>
                <td colspan="2"><h1>Discapacidades</h1><span>{{persona._discapacidad}}</span></td>
              </tr>
              <tr class="table-banner-tr">
                <td class="last-line"><h1>Elabora</h1><span>{{_created._user}}{{#if _created.branch}} ({{_created._branch}}){{/if}}</span></td>
                <td class="last-line"><h1>Servicio Elabora</h1><span>{{_created._userService}}</span></td>
                <td class="last-line"><h1>Responsable</h1><span></span>{{_created._responsable}}</td>
                <td class="last-line"><h1>Servicio Atención</h1><span>{{_created._service}}</span></td>
              </tr>
              {{else}}
              {{/if}}
            </tbody>
          </table>
        </div>  
      </div>
      </div>  
    </div>
